{
    "configurations": [
        {
            "name": "ESP32",
            "includePath": [
                "${workspaceFolder}/main",
                "${workspaceFolder}/components/ultrasonic",
                "${workspaceFolder}/components/triac",
                "${workspaceFolder}/../limero/inc",
                "${workspaceFolder}/../limero/esp32_idf",
                "${workspaceFolder}/../ArduinoJson/src/",
                "${HOME}/esp/esp-idf/components/**",
                "${HOME}/esp/esp-idf/components/freertos/include/freertos/",
                "${HOME}/esp/xtensa-esp32-elf/**",
                "${HOME}/esp/esp-idf/components/wpa_supplicant/port/include/",
                "${HOME}/.espressif/tools/xtensa-esp32-elf/**"
            ],
            "defines": [
                "FREERTOS",
                "ESP32_IDF",
                "STEPPER_SERVO",
                "HOSTNAME=drive",
                "COMMAND",
                "MQTT_PORT=1883"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "clang-x64",
            "forcedInclude": [],
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}